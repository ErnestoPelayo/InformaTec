<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wwwwww"
    tools:context=".activity_actions">


    <Button
        android:id="@+id/btn_contestCreate"
        android:layout_width="195dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="71dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="118dp"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/btn_ConferencesCreate"
        android:layout_width="203dp"
        android:layout_height="37dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="71dp"
        android:layout_marginTop="251dp"
        android:layout_marginEnd="110dp"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/btnContestDelete"
        android:layout_width="338dp"
        android:layout_height="26dp"
        android:layout_below="@+id/btn_contestCreate"
        android:layout_alignStart="@+id/btn_ConferencesCreate"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-71dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="45dp"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/btnContestUpdate"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:layout_below="@+id/btnContestDelete"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="365dp"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/btnConferencesDelete"
        android:layout_width="212dp"
        android:layout_height="33dp"
        android:layout_below="@+id/btn_ConferencesCreate"
        android:layout_alignStart="@+id/btn_ConferencesCreate"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-4dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="104dp"
        android:background="@color/colorAccent" />

    <Button
        android:id="@+id/btnConferencesUpdate"
        android:layout_width="201dp"
        android:layout_height="35dp"
        android:layout_below="@+id/btnConferencesDelete"
        android:layout_alignStart="@+id/btnConferencesDelete"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="113dp"
        android:layout_marginBottom="192dp"
        android:background="@color/colorAccent" />

</RelativeLayout>